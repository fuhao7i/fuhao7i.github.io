<span class="n">flags</span><span class="p">.</span><span class="n">DEFINE_string</span><span class="p">(</span><span class="s">'classes'</span><span class="p">,</span> <span class="s">'./data/labels/coco.names'</span><span class="p">,</span> <span class="s">'path to classes file'</span><span class="p">)</span>
